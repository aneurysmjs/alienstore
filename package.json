{
  "name": "alienstore",
  "version": "0.0.0-development",
  "repository": "git@github.com:aneurysmjs/alienstore.git",
  "author": "jero <aneurysmjs@outlook.com>",
  "license": "MIT",
  "scripts": {
    "clean": "./node_modules/.bin/rimraf dist es",
    "build": "rollup -c",
    "commit": "git-cz",
    "test": "jest",
    "semantic-release": "semantic-release",
    "lint": "eslint './src/**/*.{ts,tsx}'",
    "pretest": "yarn build",
    "prepare": "yarn clean clean && yarn lint && yarn test",
    "eslint-check": "eslint --print-config src/index.ts | eslint-config-prettier-check"
  },
  "keywords": [
    "alienStore",
    "alien store",
    "react",
    "redux",
    "useAlien hook",
    "useWithAlien hook",
    "redux lazy loading",
    "redux code splitting"
  ],
  "main": "dist/uselazy.js",
  "unpkg": "dist/uselazy.js",
  "module": "es/uselazy.js",
  "files": [
    "dist",
    "lib",
    "es"
  ],
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "@testing-library/react-hooks": "3.2.1",
    "@types/jest": "25.1.3",
    "@types/node": "13.7.0",
    "@types/react": "16.9.19",
    "@types/react-redux": "7.1.7",
    "@types/testing-library__react": "9.1.2",
    "@typescript-eslint/eslint-plugin": "2.19.0",
    "@typescript-eslint/parser": "2.19.0",
    "chalk": "3.0.0",
    "commitizen": "4.0.3",
    "cross-env": "7.0.0",
    "cz-conventional-changelog": "3.1.0",
    "eslint": "6.8.0",
    "eslint-config-airbnb-base": "14.0.0",
    "eslint-config-prettier": "6.10.0",
    "eslint-plugin-class-property": "1.1.0",
    "eslint-plugin-import": "2.20.1",
    "eslint-plugin-prettier": "3.1.2",
    "eslint-plugin-react": "7.18.3",
    "eslint-plugin-react-hooks": "2.3.0",
    "jest": "25.1.0",
    "prettier": "1.19.1",
    "react": "16.12.0",
    "react-test-renderer": "16.12.0",
    "rimraf": "3.0.2",
    "rollup": "1.31.0",
    "rollup-plugin-commonjs": "10.1.0",
    "rollup-plugin-node-resolve": "5.2.0",
    "rollup-plugin-replace": "2.2.0",
    "rollup-plugin-terser": "5.2.0",
    "rollup-plugin-typescript2": "0.25.3",
    "semantic-release": "^17.0.2",
    "ts-jest": "25.2.1",
    "typescript": "3.7.5"
  },
  "peerDependencies": {
    "react": "16.12.0",
    "redux": "4.0.5"
  },
  "dependencies": {
    "react-dom": "16.12.0",
    "react-redux": "7.2.0",
    "uselazy": "5.1.6"
  }
}
